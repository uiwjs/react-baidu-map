{
  "name": "@uiw/react-baidu-map",
  "version": "1.19.4",
  "description": "Baidu Map Components for React.",
  "author": "kenny wong <wowohoo@qq.com>",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "prepare": "husky install && npm run build",
    "released": "npm run build && npm run doc",
    "doc": "kkt build --app-src ./website && npm run map",
    "start": "kkt start --app-src ./website",
    "watch": "tsbb watch & npm run css:watch",
    "build": "tsbb build && npm run css:build",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage",
    "css:build": "compile-less -d src -o esm",
    "css:watch": "compile-less -d src -o esm --watch",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "map": "source-map-explorer build/static/js/*.js --html build/website-result.html"
  },
  "homepage": "https://uiwjs.github.io/react-baidu-map",
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-baidu-map"
  },
  "files": [
    "esm",
    "cjs",
    "lib",
    "src"
  ],
  "keywords": [
    "react-baidu-map",
    "baidu",
    "map",
    "react",
    "react-bmap",
    "baidu-map",
    "bmap",
    "bdmap",
    "百度",
    "地图",
    "百度地图"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "jest": {},
  "peerDependencies": {
    "@babel/runtime": ">=7.12.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.15.4",
    "@kkt/less-modules": "6.11.0",
    "@kkt/raw-modules": "6.11.0",
    "@kkt/scope-plugin-options": "6.11.0",
    "@loadable/component": "5.15.0",
    "@types/loadable__component": "5.13.4",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "@types/react-test-renderer": "17.0.1",
    "@uiw/react-code-preview": "2.0.0",
    "@uiw/react-github-corners": "1.5.3",
    "@uiw/react-markdown-preview": "3.3.2",
    "@uiw/react-domain-verify": "1.1.1",
    "@uiw/reset.css": "1.0.5",
    "@uiw/react-loader": "4.9.6",
    "compile-less-cli": "1.8.8",
    "cross-env": "7.0.3",
    "jest-environment-jsdom": "27.1.1",
    "husky": "7.0.2",
    "kkt": "6.11.0",
    "lint-staged": "11.1.2",
    "prettier": "2.4.0",
    "rehype-attr": "2.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "react-test-renderer": "17.0.2",
    "source-map-explorer": "2.5.2",
    "tsbb": "3.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
